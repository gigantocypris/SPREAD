dispatch {
  step_list = input model_scaling modify filter errors_premerge scale \
              postrefine \
              statistics_unitcell statistics_beam model_statistics statistics_resolution \
              group errors_merge statistics_intensity merge statistics_intensity_cxi
}
output {
  do_timing=True
  save_experiments_and_reflections=True
  output_dir=.
  #prefix=
  log_level=0
}

filter.outlier.min_corr=-1
filter.algorithm=unit_cell
filter.unit_cell.value.relative_length_tolerance=0.03
